package system;

class Version
{
	int MAJOR;
	int MINOR;
	int PATCH;
}

public class Settings {

	String projectName;
	Version version;
	
	public void setProjectName(String name)
	{
		projectName=name;
	}
	
	public void setVersion(int M, int m, int p)
	{
		version.MAJOR=M;
		version.MINOR=m;
		version.PATCH=p;
	}
	
	
}
