package system;

import java.util.ArrayList;

class Version
{
	int MAJOR;
	int MINOR;
	int PATCH;
}

/*
 * Stores all project information
 * 
 */
public class Settings {

	//project information
	String projectName;
	Version version;
	
	//file tracking
	ArrayList <String> ProjectFiles = null;
	
	public Settings()
	{
		
	}
	
	public void setProjectName(String name)
	{
		projectName=name;
	}
	
	public void setVersion(int M, int m, int p)
	{
		version.MAJOR=M;
		version.MINOR=m;
		version.PATCH=p;
	}
	

}
